{
  "name": "app",
  "version": "0.0.1",
  "description": "A simple example using webpack and to build an application to both electron and web targets",
  "main": "index.js",
  "scripts": {
    "electron": "electron .",
    "web": "webpack --target web && webpack-dev-server --target web ",
    "dev": "webpack-dev-server --config webpack.config.js --progress --env.isDev",
    "start": "parallelshell \"nodemon server.js\" \"npm run dev\" \"npm run electron\"",
    "clean": "rimraf dist/*",
    "build": "npm run clean && node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --config webpack.config.js --progress --bail",
    "package:win": "electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=node_modules --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=win32 --arch=all --overwrite "
  },
